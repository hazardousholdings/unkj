<?xml version="1.0" encoding="utf-8" ?>
<application-config xmlns="http://unkj.hazardousholdings.com/configuration/app"
			 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			 xsi:schemaLocation="http://unkj.hazardousholdings.com/configuration/app app-config.xsd">

	<configuration>
		<database key="ska">
			<class-name>org.postgresql.Driver</class-name>
			<connection-string>jdbc:postgresql://localhost:5432/unkj</connection-string>
			<username>ska</username>
			<password>ska</password>
		</database>
		<config key="log-level">WARNING</config>
	</configuration>

	<groups>
		<group>admins</group>
	</groups>

</application-config>
